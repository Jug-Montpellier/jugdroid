<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.jug.montpellier.app.jugdroid"
    android:versionCode="1"
    android:versionName="1.0">
    <uses-sdk android:minSdkVersion="4" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:icon="@drawable/icon" android:theme="@style/Theme.JugDroid" android:name=".JugdroidApplication" android:label="@string/app_name">
        <activity android:name=".ui.DashboardActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MembersActivity_" android:label="@string/speakers"></activity>
        <activity android:name=".ui.MemberDetailActivity_" android:label="@string/speaker_details"></activity>
        <activity android:name=".ui.PartnersActivity_" android:label="@string/partners"></activity>
        <activity android:name=".ui.PartnerDetailActivity_" android:label="@string/partners_details"></activity>
</application>
</manifest>

