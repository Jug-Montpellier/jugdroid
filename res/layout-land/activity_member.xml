<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <fragment
        android:name="org.jug.montpellier.app.jugdroid.MemberListFragment"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:id="@+id/member_list_fragment"
        android:layout_weight="25">
    </fragment>
    <fragment
        android:name="org.jug.montpellier.app.jugdroid.MemberDetailFragment"
        android:layout_width="0dp"
        android:layout_height="fill_parent"
        android:id="@+id/member_detail_fragment"
        android:layout_weight="75">
    </fragment>
</LinearLayout>