<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/partners_list_frame" android:layout_height="match_parent"
		android:layout_weight="40" android:background="@drawable/listview_rounded_rect"
		android:paddingLeft="2dip" android:paddingRight="2dip"
		android:paddingTop="12dip" android:paddingBottom="12dip"
		android:layout_width="0px" android:layout_marginLeft="5dip"
		android:layout_marginRight="2dip" android:layout_marginBottom="10dip"
		android:layout_marginTop="10dip">
	</FrameLayout>
	<FrameLayout android:id="@+id/partner_detail_frame"
		android:layout_weight="60" android:layout_width="0px"
		android:layout_height="fill_parent" />
</LinearLayout>